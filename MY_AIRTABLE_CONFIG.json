  },
  "scoring": {
    "enabled": true,
    "icpCriteria": "B2B SaaS companies, 50-500 employees, uses modern tech stack like React/Node.js, has LLM/AI integration needs",
    "weights": {
      "hasEmail": 20,
      "hasPhone": 15,
      "socialPresence": 15,
